<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mvvm.view.controllers.CurrenciesController">
   <children>
      <TableView fx:id="table" layoutX="202.0" layoutY="100.0" prefHeight="200.0" prefWidth="252.0">
        <columns>
          <TableColumn fx:id="tableName" prefWidth="119.0" text="Currency name" />
          <TableColumn fx:id="tablePrice" prefWidth="132.0" text="Price in EURO" />
        </columns>
      </TableView>
      <Label layoutX="64.0" layoutY="36.0" prefHeight="37.0" prefWidth="149.0" text="Currencies">
         <font>
            <Font name="Yu Gothic Light" size="22.0" />
         </font>
      </Label>
      <Label layoutX="213.0" layoutY="340.0" text="Add">
         <font>
            <Font name="Yu Gothic Light" size="27.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="75.0" layoutX="126.0" layoutY="322.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../favicon_io/circle-plus.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="87.0" fitWidth="243.0" layoutX="92.0" layoutY="313.0" onMouseClicked="#add" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="511.0" layoutY="340.0" text="Update">
         <font>
            <Font name="Yu Gothic Light" size="27.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="75.0" layoutX="424.0" layoutY="322.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../favicon_io/pen-circle.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="87.0" fitWidth="243.0" layoutX="390.0" layoutY="313.0" onMouseClicked="#update" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="error" layoutX="390.0" layoutY="43.0" prefHeight="37.0" prefWidth="149.0" text="Error">
         <font>
            <Font name="Yu Gothic Light" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
