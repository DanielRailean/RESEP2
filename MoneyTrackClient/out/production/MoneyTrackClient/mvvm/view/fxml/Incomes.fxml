<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mvvm.view.controllers.IncomesController">
   <children>
      <TableView fx:id="table" layoutX="27.0" layoutY="113.0" prefHeight="335.0" prefWidth="638.0">
        <columns>
            <TableColumn fx:id="tableDescription" prefWidth="198.0" text="Description" />
            <TableColumn fx:id="tableAccount" prefWidth="155.0" text="Account" />
          <TableColumn fx:id="tableAmount" prefWidth="145.0" text="Amount" />
            <TableColumn fx:id="tableCurrency" prefWidth="139.0" text="Currency" />
        </columns>
      </TableView>
      <Label layoutX="27.0" layoutY="14.0" prefHeight="37.0" prefWidth="149.0" text="Incomes">
         <font>
            <Font name="Yu Gothic Light" size="22.0" />
         </font>
      </Label>
      <Label layoutX="130.0" layoutY="476.0" text="Add">
         <font>
            <Font name="Yu Gothic Light" size="27.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="75.0" layoutX="43.0" layoutY="458.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../favicon_io/circle-plus.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="87.0" fitWidth="243.0" layoutX="9.0" layoutY="449.0" onMouseClicked="#add" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="557.0" layoutY="476.0" text="Delete">
         <font>
            <Font name="Yu Gothic Light" size="27.0" />
         </font>
      </Label>
      <ImageView fitHeight="68.0" fitWidth="75.0" layoutX="470.0" layoutY="458.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../favicon_io/trash-can.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="87.0" fitWidth="243.0" layoutX="436.0" layoutY="449.0" onMouseClicked="#delete" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="error" layoutX="28.0" layoutY="44.0" prefHeight="68.0" prefWidth="638.0" wrapText="true">
         <font>
            <Font name="Yu Gothic Light" size="22.0" />
         </font>
      </Label>
      <DatePicker fx:id="datePick" layoutX="491.0" layoutY="75.0" onHiding="#updateTable" />
   </children>
</AnchorPane>
